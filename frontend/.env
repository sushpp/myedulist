# React App Configuration
REACT_APP_API_URL=http://localhost:5000/api
REACT_APP_APP_NAME=EduList
REACT_APP_VERSION=1.0.0

# Environment
REACT_APP_NODE_ENV=development

# Feature Flags (optional - for future use)
REACT_APP_ENABLE_ANALYTICS=false
REACT_APP_ENABLE_PUSH_NOTIFICATIONS=false

# External Services (optional - for future integrations)
REACT_APP_GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here
REACT_APP_STRIPE_PUBLISHABLE_KEY=your_stripe_publishable_key_here

# Pagination and UI Settings
REACT_APP_ITEMS_PER_PAGE=12
REACT_APP_MAX_UPLOAD_SIZE=5242880
REACT_APP_DEFAULT_LANGUAGE=en

# Timeouts
REACT_APP_API_TIMEOUT=10000
REACT_APP_DEBOUNCE_DELAY=300